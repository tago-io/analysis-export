{
  "name": "template-analysis",
  "version": "1.0.0",
  "author": "Tago LLC",
  "license": "Copyright",
  "private": true,
  "scripts": {
    "test": "jest .",
    "test:watch": "jest . --watch",
    "test:coverage": "jest . --coverage",
    "linter": "eslint .",
    "build": "analysis-builder src/analysisHandler.ts ./build/handler.tago.js",
    "start": "ts-node-dev --inspect --",
    "inspect": "node --inspect ./node_modules/.bin/ts-node --files ./src/start.ts",
    "export": "ts-node-dev export/start.ts"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "commitlint": "16.2.4",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "28.0.3",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "zlib": "1.0.5",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@tago-io/builder": "3.0.3",
    "@tago-io/sdk": "10.7.7"
  }
}
